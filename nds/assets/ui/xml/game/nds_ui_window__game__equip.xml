<?xml version="1.0" encoding="ISO-8859-1"?>
<root>
    <config>
        <include sub_dir="ui/game/">
            <header path="ui/nds_ui.h"/>
            <header path="ui/ui_element.h"/>
            <header path="ui/ui_button.h"/>
            <header path="ui/ui_manager.h"/>
            <header path="vectors.h"/>
            <header path="defines.h"/>
            <header path="game.h"/>
        </include>
        <code>
            NDS_allocate_ui_for__game_hud(p_game);
        </code>
        <output base_dir="./" c_path="./nds/source/ui/game/nds_ui_window__game__equip.c" h_path="./nds/include/ui/game/nds_ui_window__game__equip.h"/>
        <platform target="NDS" size="256,256"/>
        <background layer="0" path="./core/assets/ui/default/ui_map_inventory_column.png"/>
        <background layer="1" path="./core/assets/ui/default/ui_map_equip.png"/>
        <ui_func_signature for="grid"/>
        <ui_func_signature for="child" c_signature="allocate_ui_element_from__ui_manager_as__child"/>
        <ui_func_signature for="allocate_ui" c_signature="allocate_ui_element_from__ui_manager"/>
        <ui_func_signature for="allocate_ui_container" c_signature="allocate_many_ui_elements_from__ui_manager_in__succession"/>
        <ui_func_signature for="button" c_signature="allocate_ui_element_from__ui_manager"/>
    </config>
    <ui>
        <allocate_ui name="p_slider">
            <button x="188" y="92" width="16" height="80"/>
        </allocate_ui>
        <allocate_ui name="p_button_filter__usable">
            <button x="220" y="96" width="16" height="16" color="64,196,64"/>
        </allocate_ui>
        <allocate_ui name="p_button_filter__wearable">
            <button x="220" y="124" width="16" height="16" color="64,196,64"/>
        </allocate_ui>
        <allocate_ui name="p_button_filter__consumable">
            <button x="220" y="152" width="16" height="16" color="64,196,64"/>
        </allocate_ui>
        <grid x="92" y="1" size="9" stride__y="28">
            <allocate_ui_container size="3" stride__x="24">
                <button width="23" height="27" color="55,55,96"/>
                <child>
                    <button x="2" y="2" width="18" height="22" color="196,64,64"/>
                </child>
            </allocate_ui_container>
        </grid>
    </ui>
</root>


